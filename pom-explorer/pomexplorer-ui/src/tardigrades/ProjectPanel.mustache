<div >
 <div >
  <div class='mdl-textfield mdl-js-textfield mdl-textfield--floating-label'>
   <input class='mdl-textfield__input' type='text' id='searchBox'>
    {{input}}
   </input>
   <label class='mdl-textfield__label' for='searchBox'>
    Project search...
   </label>

  </div>
  <div class='mdl-button mdl-button--icon'>
   <i class='material-icons'>
    search
   </i>

  </div>

 </div>
 <div class='projects-list'>
  <div class='project-card mdl-card mdl-shadow--2dp'>
   <div class='mdl-card__title mdl-card--expand'>
   <h2 class='mdl-card__title-text'>
    <div >
     <div >
      {{groupId}}
     </div>
     <div >
      {{artifactId}}
     </div>
     <div >
      {{version}}
     </div>

    </div>

   </h2>

   </div>
   <div class='mdl-card__supporting-text'>
    {{content}}
   </div>
   <div class='mdl-card__supporting-text' style='display:none;'>
    {{details}}
   </div>
   <div class='mdl-card__actions mdl-card--border'>
   <a class='mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'>
    Details
   </a>
   <button class='mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect'>
    <i class='material-icons'>
     mode_edit
    </i>

   </button>

   </div>
   <div class='mdl-card__menu'>
    {{menu}}
   </div>

  </div>

 </div>

</div>
